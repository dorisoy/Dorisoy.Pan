<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:converters="clr-namespace:Dorisoy.PanClient.Converters"
             xmlns:controls="clr-namespace:Dorisoy.PanClient.Controls"
             mc:Ignorable="d"
             x:Class="Dorisoy.PanClient.Pages.Upload.AddFolderView"
             xmlns:vm="using:Dorisoy.PanClient.ViewModels"
             x:CompileBindings="False"
             x:DataType="vm:AddFolderViewModel">
    <Grid Width="400"
          RowDefinitions="*">
        <TextBox  Grid.Row="0" Text="{Binding FolderName}"></TextBox>
    </Grid>
</UserControl>
