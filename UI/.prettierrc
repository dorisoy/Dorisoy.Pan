{
    "printWidth": 9999,
    "eslintIntegration": true,
    "stylelintIntegration": true,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "ignore",
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "files.insertFinalNewline": true,
    "useTabs": false,
    "endOfLine": "auto",
    "ignorePath": ".gnore",
    "trailingComma": "none"
}
